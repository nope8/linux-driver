!_TAG_FILE_FORMAT	2	/extended format; --format=1 will not append ;" to lines/
!_TAG_FILE_SORTED	1	/0=unsorted, 1=sorted, 2=foldcase/
!_TAG_PROGRAM_AUTHOR	Darren Hiebert	/dhiebert@users.sourceforge.net/
!_TAG_PROGRAM_NAME	Exuberant Ctags	//
!_TAG_PROGRAM_URL	http://ctags.sourceforge.net	/official site/
!_TAG_PROGRAM_VERSION	5.9~svn20110310	//
DRIVER_AUTHOR	hello.c	/^MODULE_AUTHOR(DRIVER_AUTHOR);$/;"	v
DRIVER_AUTHOR	hello.c	4;"	d	file:
DRIVER_DESC	hello.c	/^MODULE_DESCRIPTION(DRIVER_DESC);$/;"	v
DRIVER_DESC	hello.c	5;"	d	file:
KERNELDIR	Makefile	/^KERNELDIR ?= \/lib\/modules\/$(shell uname -r)\/build  $/;"	m
PWD	Makefile	/^PWD := $(shell pwd)$/;"	m
hello_cleanup	hello.c	/^module_exit (hello_cleanup);$/;"	v
hello_cleanup	hello.c	/^void __exit hello_cleanup()$/;"	f
hello_init	hello.c	/^int  __init hello_init()$/;"	f
hello_init	hello.c	/^module_init (hello_init);$/;"	v
obj-m	Makefile	/^    obj-m := hello.o$/;"	m
