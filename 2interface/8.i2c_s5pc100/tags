dev_read	char-dev.c	/^static int dev_read(struct file *fp, char __user *buf, size_t count, loff_t *offset)$/;"	f	file:
dev_release	char-dev.c	/^static int dev_release(struct inode *inode, struct file *file){$/;"	f	file:
dev_write	char-dev.c	/^static int dev_write(struct file *fp, const char __user *buf, size_t count, loff_t *offset)$/;"	f	file:
hello_cleanup	char-dev.c	/^module_exit (hello_cleanup);$/;"	v
